{
    "name": "jarrus90/yii2-user",
    "description": "Flexible user registration and authentication module for Yii2",
    "keywords": ["yii2", "yii2-user", "user management", "jarrus90"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/jarrus90/yii2-user"
    },
    "authors": [
        {
            "name": "Vladimir Stovbchatij",
            "email": "unreality1990@gmail.com",
            "homepage": "https://github.com/jarrus90",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-swiftmailer": "~2.0",
        "yiisoft/yii2-authclient": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0",
        "creocoder/yii2-flysystem": "0.8.*",
        "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-widget-select2": "*",
        "jarrus90/yii2-core": "*",
        "jarrus90/yii2-multilang": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "codeception/aspect-mock": "*"
    },
    "autoload": {
        "psr-4": {
            "jarrus90\\User\\": ""
        }
    },
    "extra": {
        "bootstrap": "jarrus90\\User\\Bootstrap",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}